<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane prefHeight="280.0" prefWidth="460.0" style="-fx-background-color: #c4a218;">
         <children>
            <AnchorPane id="pane" layoutX="10.0" layoutY="10.0" opacity="0.91" prefHeight="260.0" prefWidth="440.0">
               <children>
                  <ComboBox fx:id="box1" layoutX="89.0" layoutY="47.0" prefHeight="35.0" prefWidth="300.0" promptText="From" style="-fx-background-color: #c4a218;" />
                  <ComboBox fx:id="box2" layoutX="89.0" layoutY="100.0" prefHeight="35.0" prefWidth="300.0" promptText="To" style="-fx-background-color: #c4a218;">
                     <effect>
                        <SepiaTone level="0.33" />
                     </effect>
                  </ComboBox>
                  <TextField fx:id="num" layoutX="89.0" layoutY="158.0" prefHeight="35.0" prefWidth="140.0" promptText="Type ....">
                     <effect>
                        <SepiaTone level="0.0" />
                     </effect>
                     <font>
                        <Font name="Times New Roman Bold" size="13.0" />
                     </font>
                  </TextField>
                  <Label fx:id="label" layoutX="252.0" layoutY="158.0" opacity="0.6" prefHeight="35.0" prefWidth="140.0" text="Result">
                     <font>
                        <Font name="Times New Roman" size="17.0" />
                     </font>
                     <effect>
                        <DropShadow height="0.0" radius="0.0" spread="0.18" width="0.0">
                           <color>
                              <Color red="1.0" />
                           </color>
                        </DropShadow>
                     </effect>
                  </Label>
                  <Button fx:id="submit" layoutX="169.0" layoutY="205.0" mnemonicParsing="false" onAction="#actiondone" prefHeight="35.0" prefWidth="140.0" text="Convert">
                     <effect>
                        <SepiaTone level="0.59" />
                     </effect>
                     <font>
                        <Font name="Times New Roman" size="16.0" />
                     </font>
                  </Button>
               </children>
            </AnchorPane>
         </children>
      </AnchorPane>
   </children>
</GridPane>
